name: "VGG16_layers_viewpoint"
layers {
  name: "data"
  type: DATA
  top: "data"
  data_param {
    source: "/home/yan/server/syn_lmdb_engine_train_image"
    backend: LMDB
    batch_size: 64
  }
  transform_param {
    #crop_size: 224
    mirror: false
    mean_file:"imagenet_mean.binaryproto"
  }
  include: { phase: TRAIN }

}
layers {
  name: "label"
  type: DATA
  top: "label"
  data_param {
    source: "/home/yan/server/syn_lmdb_engine_train_label"
    backend: LMDB
    batch_size: 64
  }
  include: { phase: TRAIN }
}
layers {
  name: "data"
  type: DATA
  top: "data"
  data_param {
    source: "/home/yan/server/syn_lmdb_engine_test_image"
    backend: LMDB
    batch_size: 8
  }
  transform_param {
    #crop_size: 224
    mirror: false
    mean_file:"imagenet_mean.binaryproto"
  }
  include: { phase: TEST }
}
layers {
  name: "label"
  type: DATA
  top: "label"
  data_param {
    source: "/home/yan/server/syn_lmdb_engine_test_label"
    backend: LMDB
    batch_size: 8
  }
  include: { phase: TEST }
}

layers {
  bottom: "data"
  top: "conv1_1"
  name: "conv1_1"
  type: CONVOLUTION
  blobs_lr: 0
  blobs_lr: 0
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}
layers {
  bottom: "conv1_1"
  top: "conv1_1"
  name: "relu1_1"
  type: RELU
}
layers {
  bottom: "conv1_1"
  top: "conv1_2"
  name: "conv1_2"
  type: CONVOLUTION
  blobs_lr: 0
  blobs_lr: 0
