layers {
  name: "fc8-new"
  type: INNER_PRODUCT
  bottom: "fc7-new"
  top: "fc8-new"
  blobs_lr: 1
  blobs_lr: 1
  weight_decay: 1
  weight_decay: 0
  inner_product_param {
    num_output: 2352
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layers {
  name: "loss_sphere-new"
#  type: SOFTMAX_WITH_SPHERE_LOSS
  type: SOFTMAX_LOSS
