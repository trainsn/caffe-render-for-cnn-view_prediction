#    bandwidth: 6
#    sigma: 6
#  }
  bottom: "fc8-new"
  bottom: "label"
  top: "loss_sphere-new"
#  loss_weight: 1
}

layers {
  name: "accuracy_sphere"
  type: ACCURACY_SPHERE
  bottom: "fc8-new"
  bottom: "label"
  top: "accuracy_sphere"
  accuracy_sphere_param {
    tol_angle: 15
  }
